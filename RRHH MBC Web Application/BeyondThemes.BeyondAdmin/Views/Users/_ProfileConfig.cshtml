@model Model.UserModel
<div class="col-xs-12">
    <div class="tabbable">
        <ul class="nav nav-tabs" id="myTab11" style="">
            <li class="active">
                <a data-toggle="tab" href="#info-tab">
                    Información
                </a>
            </li>
            <li class="tab-red">
                <a data-toggle="tab" href="#roles-tab">
                    Roles
                </a>
            </li>
        </ul>
        <div class="tab-content tabs-flat" style="margin:0px; ">
            <div id="info-tab" class="tab-pane in active">
                @{ Html.RenderPartial("/Views/Users/_ProfileConfigInfo.cshtml", new Model.UpdateUserModel(Model.user));}
            </div>
            <div id="roles-tab" class="tab-pane">
                <div class="row">
                    <div class="col-xs-12">
                        @{ Html.RenderPartial("/Views/Users/_ProfileConfigRoles.cshtml", new Model.UserRolesModel(Model.user.user_id));}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

